{
  "name": "ReactNativeSeed",
  "version": "1.2.0",
  "private": true,
  "devDependencies": {
    "@types/jest": "22.1.2",
    "@types/react": "16.0.37",
    "@types/react-native": "0.52.9",
    "@types/react-native-vector-icons": "4.4.3",
    "@types/react-navigation": "1.0.32",
    "babel-eslint": "8.2.1",
    "babel-jest": "22.2.2",
    "babel-preset-react-native": "4.0.0",
    "babel-preset-react-native-stage-0": "^1.0.1",
    "concurrently": "3.5.1",
    "eslint": "4.14.0",
    "husky": "0.14.3",
    "jest-react-native": "^18.0.0",
    "react-test-renderer": "16.2.0",
    "rimraf": "^2.6.2",
    "ts-jest": "22.0.4",
    "tslint": "5.9.1",
    "typescript": "2.6.2"
  },
  "scripts": {
    "start": "react-native start",
    "jest": "node node_modules/jest/bin/jest.js",
    "watch": "npm run build -- -w",
    "ios": "react-native run-ios",
    "android": "react-native run-android",
    "tsc": "tsc",
    "clean": "rimraf artifacts",
    "build": "npm run clean && npm run tsc --",
    "lint": "tslint src/**/*.ts",
    "test": "tslint src/**/*.tsx && eslint .",
    "precommit": "yarn test"
  },
  "jest": {
    "preset": "react-native",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "^.+\\.(js)$": "<rootDir>/node_modules/babel-jest",
      "\\.(ts|tsx)$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "transformIgnorePatterns": [
      "node_modules/(?!(react-native|lottie-react-native|expo|react-native-maps|react-native-svg|react-native-branch|native-base-shoutem-theme|react-native-easy-grid|react-native-drawer|react-native-vector-icons|react-native-keyboard-aware-scroll-view|react-navigation|native-base|@expo|react-native-scrollable-tab-view)/)"
    ]
  },
  "dependencies": {
    "lodash": "4.17.5",
    "mobx": "3.5.1",
    "mobx-react": "4.4.1",
    "native-base": "2.3.8",
    "react": "16.2.0",
    "react-native": "0.52.0",
    "react-navigation": "1.0.0-beta.27"
  }
}
